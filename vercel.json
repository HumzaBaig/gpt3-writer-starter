{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next",
      "config": {
        "env": {
          "API_CALL_TIMEOUT": "30000"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "headers": {
        "timeout": "${API_CALL_TIMEOUT}"
      }
    }
  ]
}
